class Student:
    def __init__(self, name, roll_no, marks):
        self.name = name
        self.roll_no = roll_no
        self.marks = marks
Student1 = Student("John", 101, 85)
print(Student1.name)
print(Student1.roll_no)
print(Student1.marks)
Student2 = Student("Mary", 102, 90)
print(Student2.name)
print(Student2.roll_no)
print(Student2.marks)
